{
  "moduleFileExtensions": [
    "ts",
    "js",
    "json"
  ],
  "testMatch": [
    "<rootDir>/test/**/*.(spec|test).js"
  ],
  "testEnvironment": "node",
  "reporters": [ "default" ],
  "coveragePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/coverage/",
    "<rootDir>/test/"
  ]
}
